<div class="container">
  <div class="d-flex justify-content-between align-items-center">
    <h1>My lists</h1>
    <%= link_to "Add list", new_list_path, class: "btn btn-secondary rounded-pill" %>
  </div>
  <% @lists.each do |list|  %>
    <div class="card">
      <p><%= link_to "#{list.name}", list_path(list)%></p>
    </div>
  <%end%>
</div>
